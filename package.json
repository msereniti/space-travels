    "@types/three": "^0.133.1",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "mobx": "^6.3.6",
    "mobx-react-lite": "^3.2.2",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-qr-code": "^2.0.3",
    "react-to-print": "^2.14.1",
    "react-transition-group": "^4.4.2",
    "three": "^0.133.1",
