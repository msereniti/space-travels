    "start": "yarn dev",
    "lint": "eslint ./dev/**/*.ts & eslint ./src/**/*.ts",
    "lint-fix": "eslint ./dev/**/*.ts --fix & eslint ./src/**/*.ts --fix",
    "ts": "tsc --noEmit --project ./tsconfig.json"
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.2.4",
    "@react-three/fiber": "^7.0.16",
    "@react-three/postprocessing": "^2.0.5",
