  "license": "MIT",
  "scripts": {
    "dev": "echo 'Proxy to port 8000 started on \n\n> http://localhost:3000\n' && node proxy.js &  esbuild ./src/app.tsx --bundle --minify --sourcemap --servedir=./public --outdir=./public/bundle",
